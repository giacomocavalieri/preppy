fn icon(attrs: List(Attribute(msg)), path: String) -> Element(msg) {
  html.svg(
    [
      attribute.class("icon"),
      attribute("viewBox", "0 0 15 15"),
      attribute("fill", "none"),
      ..attrs
    ],
    [
      svg.path([
        attribute("d", path),
        attribute("fill", "currentColor"),
        attribute("fill-rule", "evenodd"),
        attribute("clip-rule", "evenodd"),
      ]),
    ],
  )
}
